# Configuration de la base de données Cloud SQL PostgreSQL

# Nom de l'instance Cloud SQL (format: project-id:region:instance-name)
# Exemple: merval-depenses-app:us-central1:merval-db
INSTANCE_CONNECTION_NAME=your-project:your-region:your-instance

# Utilisateur de la base de données
DB_USER=postgres

# Mot de passe de la base de données
DB_PASSWORD=your-password-here

# Nom de la base de données
DB_NAME=merval_depenses

# Pour le développement local uniquement
DB_HOST=localhost
DB_PORT=5432

# Environment
NODE_ENV=development

